{
  "name": "auto-handoff-generator",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "esbuild code.ts --bundle --outfile=code.js --target=es2017 --format=iife",
    "build:modular": "esbuild src/main.ts --bundle --outfile=code.js --target=es2017 --format=iife",
    "build:check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix .",
    "watch": "esbuild code.ts --bundle --outfile=code.js --target=es2017 --format=iife --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
